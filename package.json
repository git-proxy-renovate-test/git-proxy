{
  "name": "@finos/git-proxy",
  "version": "1.7.1",
  "description": "Deploy custom push protections and policies on top of Git.",
  "scripts": {
    "cli": "node ./packages/git-proxy-cli/index.js",
    "client": "vite --config vite.config.js",
    "clientinstall": "npm install --prefix client",
    "server": "node index.js",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "build": "vite build",
    "test": "NODE_ENV=test mocha --exit",
    "test-coverage": "nyc npm run test",
    "test-coverage-ci": "nyc --reporter=lcovonly --reporter=text npm run test",
    "prepare": "node ./scripts/prepare.js",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\" \"test/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\" \"test/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} test/**/*.{js,jsx,ts,tsx,json} --config ./.prettierrc",
    "gen-schema-doc": "node ./scripts/doc-schema.js",
    "cypress:run": "cypress run"
  },
  "bin": {
    "git-proxy": "./index.js",
    "git-proxy-all": "concurrently 'npm run server' 'npm run client'"
  },
  "workspaces": [
    "./packages/git-proxy-cli"
  ],
  "author": "Paul Groves",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/finos/git-proxy"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "4.11.3",
    "@primer/octicons-react": "^19.14.0",
    "@seald-io/nedb": "^4.0.4",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "bit-mask": "^1.0.2",
    "body-parser": "^1.20.3",
    "classnames": "2.5.1",
    "concurrently": "^9.1.2",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "diff2html": "^3.4.51",
    "express": "^4.21.2",
    "express-http-proxy": "^2.1.1",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.1",
    "history": "5.3.0",
    "isomorphic-git": "^1.29.0",
    "jsonschema": "^1.5.0",
    "load-plugin": "^6.0.3",
    "lodash": "^4.17.21",
    "lusca": "^1.7.0",
    "moment": "^2.30.1",
    "mongodb": "^5.9.2",
    "nodemailer": "^6.9.16",
    "parse-diff": "^0.11.1",
    "passport": "^0.7.0",
    "passport-activedirectory": "^1.4.0",
    "passport-local": "^1.0.0",
    "perfect-scrollbar": "^1.5.6",
    "prop-types": "15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-html-parser": "^2.0.2",
    "react-router-dom": "6.28.1",
    "simple-git": "^3.27.0",
    "uuid": "^11.0.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.26.5",
    "@babel/preset-react": "^7.26.3",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@vitejs/plugin-react": "^4.3.4",
    "chai": "^4.5.0",
    "chai-http": "^4.4.0",
    "cypress": "^13.17.0",
    "eslint": "^8.57.1",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.2.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^9.1.7",
    "mocha": "^10.8.2",
    "nyc": "^17.1.0",
    "prettier": "^3.4.2",
    "sinon": "^19.0.2",
    "vite": "^4.5.5"
  },
  "optionalDependencies": {
    "@esbuild/darwin-arm64": "^0.24.2",
    "@esbuild/darwin-x64": "^0.24.2",
    "@esbuild/linux-x64": "0.24.2",
    "@esbuild/win32-x64": "0.24.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
